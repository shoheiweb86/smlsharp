_require "basis.smi"

structure Tree =
struct
  datatype 'a tree = 
    EmptyTree
  | Node of 'a * 'a tree * 'a tree
  val copyTree : 'a tree -> 'a tree
  val height : 'a tree -> int
  val size : 'a tree -> int
  val sum : int tree -> int
  val traverse : 'a tree -> 'a list
  val mkTree : int -> int tree
end